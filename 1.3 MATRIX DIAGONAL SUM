#include <stdio.h>
int main() {
	printf("Name : V.Balaram Krishna\n");
	printf("Reg No : 192324049\n");
    int mat[3][3];
    int sum = 0;
    printf("\nEnter elements of a 3x3 matrix:\n");
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            scanf("%d", &mat[i][j]);
        }
    }
    for (int i = 0; i < 3; i++) {
        sum += mat[i][i]; 
    }
    printf("Sum of diagonal elements: %d\n", sum);
    return 0;
}
